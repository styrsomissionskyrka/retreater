{
  "name": "retreater",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install",
    "codegen": "yarn codegen:nexus && yarn codegen:graphql",
    "codegen:watch": "npx nodemon -e ts,tsx --exec 'yarn codegen'",
    "codegen:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only api/schema.ts",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:migrate": "prisma migrate dev",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
