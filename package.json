{
  "name": "retreater",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install",
    "codegen": "yarn codegen:nexus && yarn codegen:graphql",
    "codegen:watch": "npx nodemon -e ts,tsx --exec 'yarn codegen'",
    "codegen:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only api/schema.ts",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:migrate": "prisma migrate dev",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@auth0/nextjs-auth0": "^1.3.1",
    "@fransvilhelm/feather": "^2.0.0",
    "@fransvilhelm/hooks": "^3.0.0",
    "@prisma/client": "^2.24.0",
    "@reach/auto-id": "^0.15.0",
    "@reach/rect": "^0.15.0",
    "apollo-server-micro": "^2.25.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dataloader": "^2.0.0",
    "date-fns": "^2.22.1",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "next": "^11.0.0",
    "nexus": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.5.1",
    "react-hook-form": "^7.8.3",
    "react-hot-toast": "^2.0.0",
    "react-mde": "^11.5.0",
    "react-remove-scroll": "^2.4.2",
    "react-table": "^7.7.0",
    "slug": "^5.0.1",
    "stripe": "^8.154.0",
    "zod": "^3.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/named-operations-object": "^1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.6",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-codegen/typescript-react-apollo": "2.2.5",
    "@tabler/icons": "^1.41.2",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/classnames": "^2.3.1",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^15.6.2",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-table": "^7.7.1",
    "@types/slug": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "cypress": "^7.4.0",
    "cypress-nextjs-auth0": "^2.0.1",
    "eslint": "^7.27.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "faker": "^5.5.3",
    "fishery": "^1.3.1",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "prisma": "^2.24.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,jsx,tsx,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 120
  }
}
