    "@types/slug": "^5.0.2",
    "@types/wicg-native-file-system": "^2020.6.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^8.2.0",
    "cypress-nextjs-auth0": "^2.0.1",
