    "micro-cors": "^0.1.1",
    "next": "^11.1.0",
    "nexus": "^1.1.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.5.2",
    "react-hook-form": "^7.12.2",
    "react-hot-toast": "^2.1.0",
    "react-mde": "^11.5.0",
    "react-remove-scroll": "^2.4.3",
    "react-table": "^7.7.0",
    "remark-frontmatter": "^4.0.0",
    "remark-html": "^14.0.0",
    "remark-parse": "^10.0.0",
    "remark-stringify": "^10.0.0",
